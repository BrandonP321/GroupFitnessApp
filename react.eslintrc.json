{
    "env": {
        "es2021": true
    },
    "extends": [
        "./global.eslintrc.json",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "settings": {
        "import/resolver": {
            "typescript": {},
            "babel-plugin-root-import": {
              "rootPathPrefix": "~/",
              "rootPathSuffix": "./"
            }
          }
    }
}
