{
    "env": {
        "es2021": true
    },
    "extends": [],
    "parserOptions": {
        "ecmaFeatures": {},
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "no-duplicate-imports": "warn"
    },
    "settings": {
        "import/resolver": {
            "typescript": {},
            "babel-plugin-root-import": {
                "rootPathPrefix": "~/",
                "rootPathSuffix": "./"
            }
        }
    }
}